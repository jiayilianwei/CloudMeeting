<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>

    <!-- **************** 以下是ESFramework服务器配置  ****************-->
    <!--服务器监听的端口-->
    <add key="Port" value="4538"/>  
    <!--心跳超时秒数-->
    <add key="HeartbeatTimeoutInSecs" value="30"/>
    <!--UserID的最大长度-->
    <add key="MaxLengthOfUserID" value="20"/>

    
    <!-- **************** 以下是OMCS服务器配置  ****************-->
    <!--服务器监听的端口-->
    <add key="OMCS_Port" value="4534"/>    
    <!--是否记录安全日志文件 Omcs.txt-->
    <add key="SecurityLogEnabled" value="false"/>     
  </appSettings>
</configuration>